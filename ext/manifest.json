{
  "description": "Extension",
  "manifest_version": 2,
  "name": "Hackathon",
  "version": "1.0",
  "homepage_url": "https://github.com/Midnight145/HackUNT2024",
  "background": {
      "scripts": ["main.js"]
    },
  "permissions": ["contextMenus"],
  "browser_action": {
    "default_popup": "ext.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
}